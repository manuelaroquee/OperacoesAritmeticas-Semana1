programa
{
	
	funcao inicio()
	{
	    inteiro num
		
		escreva("Digite um número inteiro e descubra se ele é impar ou par ")
		leia(num)

          
		se (num % 2 == 0) {
	    
	    	  escreva("O número " ,num, " é par")
	    }
		
	    senao {
	    	escreva("O número " ,num, " é ímpar") 
	    }

    }
}
